{
  "name": "@tokenbb/client",
  "version": "0.1.0",
  "private": true,
  "author": "Buildteam <reggaemuffin@buildteam.io>",
  "scripts": {
    "serve": "vue-cli-service serve --host 0.0.0.0 --port 3000",
    "build": "vue-cli-service build",
    "test": "npm run lint",
    "lint": "vue-cli-service lint",
    "deploy": "bash scripts/deploy.sh lab",
    "pretest": "vue-cli-service lint --no-fix",
    "start": "npm run serve",
    "start_production": "node -v && node --harmony --experimental-modules --no-warnings server.mjs"
  },
  "dependencies": {
    "@steemit/steem-js": "^0.7.4",
    "await-timeout": "^0.3.0",
    "buefy": "^0.7.2",
    "bulma-extensions": "^4.0.1",
    "jwt-decode": "^2.2.0",
    "koa": "^2.7.0",
    "koa-no-cache": "^2.0.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "koa-static-cache": "^5.1.2",
    "mavon-editor": "git+https://github.com/BuildTeamDev/mavonEditor.git",
    "mini-toastr": "^0.8.1",
    "moment": "^2.24.0",
    "register-service-worker": "^1.5.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "secure-random": "git+https://github.com/BuildTeamDev/secure-random.git",
    "vue": "^2.5.22",
    "vue-notifications": "^0.9.0",
    "vue-router": "^3.0.2",
    "vue-sanitize": "^0.2.0",
    "vue2-editor": "^2.6.6",
    "vuex": "^3.1.0",
    "xhr": "^2.5.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-pwa": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.13.0",
    "eslint-plugin-vue": "^5.1.0",
    "lint-staged": "^8.1.3",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.22"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "browser": {
    "request": "xhr"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
